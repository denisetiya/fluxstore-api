// create db fluxstore

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

// add user table part

model User  {
  id String @id @default(cuid())
  name String
  email String @unique
  password String
  address String?
  profilePic String?
}


